<!--pages/detail-menu/detail-menu.wxml-->
<view class="all-menu">
    <block wx:for="{{songMenus}}" wx:key="cat">
        <view>
            <area-header title="{{item.cat}}" hasMore="{{false}}" />
            <view class="list">
                <block wx:for="{{item.playlists}}" wx:key="id" wx:for-item="iten">
                    <view class="iten">
                        <song-menu-item itemData="{{iten}}" />
                    </view>
                </block>   
            </view>
        </view>
    </block>
</view>